{
  "name": "sync-expense",
  "version": "1.0.0",
  "description": "Web app that automates the process of tracking shared expenses with the power of image-to-PDF conversion and synchronization with a Google Sheet.",
  "main": "server.ts",
  "type": "module",
  "scripts": {
    "start": "node server_dist/server.js",
    "server": "tsc -p tsconfig.json -w | nodemon server_dist/server.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "npm ci && tsc -p tsconfig.json",
    "build:client": "cd client && npm ci && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathias-onu/sync-expense.git"
  },
  "author": "Mathias Onu",
  "license": "ISC",
  "dependencies": {
    "@types/heic-convert": "^1.2.3",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "googleapis": "^134.0.0",
    "heic-convert": "^2.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.1",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.14.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.25",
    "@types/pdfkit": "^0.13.4",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.2"
  }
}
